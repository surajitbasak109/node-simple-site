<%- contentFor('hero') %>
<section class="hero">
	<div class="hero-content">
		<h2>All about us!</h2>
		<p>We're are small team.</p>

		<a href="/Contact" class="btn btn-lg btn-primary">Drop us a line</a>
	</div>
</section>

<%- contentFor('body') %>
<div class="jumbotron text-center">
	<ul class="list-group">
		<h1>The Team</h1>
		<li class="row">
			<% for (user of users) { %>
				<div class="col-sm-3">
					<h2><%= user.name %></h2>
					<img src="<%= user.avatar %>" alt="" class="img-thumbnail img-rounded" height="400">
				</div>
			<% } %>
		</li>
	</ul>
</div>